{
  "name": "flowstate-ai-web3",
  "version": "3.0.0",
  "description": "Web3-Native AI Assistant - ChatGPT Beauty + Claude Power + Gemini Multimodal + GenSpark Research + Voice",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "tsx watch src/server.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:worker": "wrangler dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc && tsc-alias",
    "build:frontend": "cd frontend && npm run build",
    "build:worker": "esbuild src/workers/index.ts --bundle --format=esm --outfile=dist/worker.js --platform=browser --target=es2022 --external:node:*",
    "start": "node dist/server.js",
    "deploy": "npm run build && ./cloudflare-deploy.sh",
    "deploy:worker": "wrangler deploy",
    "deploy:pages": "cd frontend && wrangler pages deploy dist --project-name=flowstate-ai",
    "deploy:full": "npm run build:worker && wrangler deploy && cd frontend && npm run build && wrangler pages deploy dist --project-name=flowstate-ai",
    "test": "vitest",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "openai": "^4.20.0",
    "@anthropic-ai/sdk": "^0.9.1",
    "@google/generative-ai": "^0.1.3",
    "ethers": "^6.9.0",
    "web3": "^4.3.0",
    "ipfs-http-client": "^60.0.1",
    "axios": "^1.6.2",
    "sharp": "^0.33.0",
    "multer": "^1.4.5-lts.1",
    "redis": "^4.6.11",
    "prisma": "^5.7.0",
    "@prisma/client": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "winston": "^3.11.0",
    "bull": "^4.12.0",
    "ws": "^8.14.2",
    "nanoid": "^5.0.4",
    "cheerio": "^1.0.0-rc.12",
    "vm2": "^3.9.19",
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "@modelcontextprotocol/sdk": "^0.5.0",
    "hono": "^3.11.7",
    "wrangler": "^3.19.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.11",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@cloudflare/workers-types": "^4.20231218.0",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "tsc-alias": "^1.8.8",
    "esbuild": "^0.19.9",
    "vitest": "^1.0.4",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
